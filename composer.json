{
  "name": "novembit/wp-i18n",
  "description": "WordPress i18n translation plugin.",
  "keywords": [
    "i18n",
    "multilingual"
  ],
  "license": [
    "GPL-3.0-or-later"
  ],
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2",
    "ext-dom": "*",
    "ext-mbstring": "*",
    "ext-json": "*",
    "guzzlehttp/guzzle": "6.3.3",
    "novembit/i18n": "dev-split_localization",
    "diazoxide/wp-lib-option": "dev-master",
    "select2/select2": "dev-master"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "wp-coding-standards/wpcs": "*"
  },
  "autoload": {
    "psr-4": {
      "NovemBit\\wp\\plugins\\i18n\\": "includes/"
    }
  },
  "scripts": {
    "style": "phpcs --standard=PSR12"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
